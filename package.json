{
    "name": "mela-decoder",
    "version": "1.0.0",
    "description": "Decode recipe files exported from the iOS app Mela",
    "keywords": [
        "mela",
        "recipe"
    ],
    "repository": "markmals/mela-decoder-js",
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs"
        }
    },
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "dev": "vitest dev",
        "test": "vitest run",
        "prepack": "unbuild",
        "release": "npm run build && np"
    },
    "dependencies": {
        "adm-zip": "^0.5.9"
    },
    "devDependencies": {
        "@types/adm-zip": "^0.5.0",
        "np": "latest",
        "prettier": "latest",
        "typescript": "latest",
        "unbuild": "latest",
        "vitest": "latest"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "semi": false,
        "arrowParens": "avoid"
    },
    "np": {
        "yarn": false
    }
}
